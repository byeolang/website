{
  "output": {
    "file_path": "website-pack-codebase-by-repomix.md",
    "style": "markdown",
    "remove_comments": false,
    "remove_empty_lines": false,
    "top_files_length": 5,
    "show_line_numbers": false,
    "copy_to_clipboard": false,
    "include_empty_directories": false,
    "calculate_tokens": false,
    "show_file_stats": true,
    "show_directory_structure": true,
    "parsable_style": false,
    "truncate_base64": false,
    "stdout": false,
    "include_diffs": false
  },
  "security": {
    "enable_security_check": false,
    "exclude_suspicious_files": true
  },
  "compression": {
    "enabled": false,
    "keep_signatures": true,
    "keep_docstrings": true,
    "keep_interfaces": true
  },
  "ignore": {
    "custom_patterns": [
        "doc/idea/*",
        "doc/note/*",
        "doc/guide/en/*",
        "doc/ref/en/*",
        "_site/**",
        "vendor/**",
        "node_modules/**",
        ".git/**",
        "*.log",
        "*.tmp"
    ],
    "use_gitignore": true,
    "use_default_ignore": true
  },
  "include": [
    "_config.yml",
    "_data/ui-text.yml",
    "_includes/footer.html",
    "_includes/masthead.html",
    "_includes/scripts.html",
    "_includes/sidebar.html",
    "_layouts/default.html",
    "_sass/minimal-mistakes.scss",
    "_sass/minimal-mistakes/_variables.scss",
    "_sass/minimal-mistakes/skins/_mint.scss",
    "assets/css/download.css",
    "assets/css/main.scss",
    "assets/js/_main.js",
    "assets/js/bg.js",
    "assets/js/download.mjs",
    "assets/js/lang-ko.mjs",
    "assets/js/lang-toggle.mjs",
    "assets/js/lightSkyBg.mjs",
    "download/index.html"
  ]
}